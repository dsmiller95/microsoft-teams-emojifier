{
    "name": "Microsoft Teams Emojinator",
    "version": "1.3",
    "description": "Adds emoji to microsoft teams via an external emoji server!",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://teams.microsoft.com.us3.cas.ms/*"
            ],
            "run_at": "document_idle",
            "js": [
                "payload.js"
            ]
        }
    ],
    "icons": {
        "16": "thonk16.png",
        "32": "thonk32.png",
        "48": "thonk48.png",
        "128": "thonk128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}